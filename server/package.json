{
  "name": "dapviz",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others npm:dev:*",
    "dev:js": "vite",
    "dev:rust": "cargo watch -C src-backend/ -- RUST_LOG=\"debug\" LOG_OUTPUT=\"stderr\" cargo run -- -p 8080 --language c-sharp ../../playground/csharp/bin/Debug/net9.0/csharp.dll",
    "build": "npm run build:js && npm run build:rust",
    "build:js": "tsc && vite build",
    "build:rust": "cargo build --release --manifest-path src-backend/Cargo.toml",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@react-three/drei": "^9.115.0",
    "@react-three/fiber": "^8.17.10",
    "@types/three": "^0.169.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-singlefile": "^2.0.3"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
